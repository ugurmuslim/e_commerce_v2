{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/e-commerce/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/e-commerce/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/e-commerce",
  "projects": {
    "e-commerce": {
      "type": "application",
      "root": "apps/e-commerce",
      "entryFile": "main",
      "sourceRoot": "apps/e-commerce/src",
      "compilerOptions": {
        "tsConfigPath": "apps/e-commerce/tsconfig.app.json"
      }
    },
    "trendyol": {
      "type": "application",
      "root": "apps/trendyol",
      "entryFile": "main",
      "sourceRoot": "apps/trendyol/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trendyol/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "hepsiburada": {
      "type": "application",
      "root": "apps/hepsiburada",
      "entryFile": "main",
      "sourceRoot": "apps/hepsiburada/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hepsiburada/tsconfig.app.json"
      }
    },
    "n11": {
      "type": "application",
      "root": "apps/n11",
      "entryFile": "main",
      "sourceRoot": "apps/n11/src",
      "compilerOptions": {
        "tsConfigPath": "apps/n11/tsconfig.app.json"
      }
    },
    "jobs": {
      "type": "application",
      "root": "apps/jobs",
      "entryFile": "main",
      "sourceRoot": "apps/jobs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/jobs/tsconfig.app.json"
      }
    }
  }
}